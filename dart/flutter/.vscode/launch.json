{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [

    {
      "name": "Dart",
      "cwd": "dart",
      "type": "dart",
      "request": "launch",
      "program": "${file}"
    },
    {
      "name": "flutter",
      "cwd": "dart/flutter",
      "request": "launch",
      "type": "dart",
      "env": {
        "DISPLAY":"localhost:10.0"
      },
      "args": ["-d", "chrome"]
    },
    {
      "name": "flutter (profile mode)",
      "cwd": "dart/flutter",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "flutter (release mode)",
      "cwd": "dart/flutter",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}